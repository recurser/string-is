    "hjson": "^3.2.2",
    "js-base64": "^3.7.5",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "next": "12.3.1",
    "next-translate": "^2.0.6",
    "next-translate-plugin": "^2.0.5",
    "papaparse": "^5.4.1",
    "prettier": "^3.2.4",
    "prismjs": "^1.29.0",
